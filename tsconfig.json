{
    "compilerOptions": {
      "target": "es5",
      "module": "CommonJS",
      "types": [
          "node"
      ],
      "lib": [
        "es6",
        "dom",
        "esnext"
      ],
      "declaration": true,
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "noImplicitAny": false,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "forceConsistentCasingInFileNames": true,
      "jsx": "react",
      "downlevelIteration": true,
      "allowJs": true
    },
    "exclude": [
      "node_modules",
      "__tests__"
    ]
  }